<script type="text/javascript">
$(window).scroll(function(){
	var s = $(window).scrollTop(),f = $('.footer').height(),wH = $(window).height(),f_top = $('.footer').offset();
	if(s > (f_top.top-(wH+60)) ){
		$('.mobQuick').addClass('posAsol');
