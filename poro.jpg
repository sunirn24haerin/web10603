<script type="text/javascript">
<!--
//dim height 조절
function layerPopBg() {
	var wrapHeight = $(document).outerHeight();
	$(".dim.on").css("height", (wrapHeight));
};
$(window).scroll(function() {
	layerPopBg();
});

function fnSelfAuth() {
	if(!$('.dim').is(':visible')){
		$('.dim').addClass('on');
		$('.layer_pop').addClass('on');
		layerPopBg();
	} else {
		$('.dim').removeClass('on');
		$('.layer_pop').removeClass('on');
		layerPopBg();
	}
}

$(document).ready(function(){
	layerPopBg();

	var reqSms = false;
	var checkSms = false;
	
	//SMS인증 요청
	$('#reqSmsBtn').click(function() {
		if(!_isValidPhoneNo()) {
			return false;
		}
	
		reqSms = false;
		checkSms = false;
		
		$.ajax({
			url: '/pororo/selfAuthentication/sms',
			type: 'POST',
			data: {},
			dataType: 'json',
			async: false,
			success: function(data) {
				if(data.CODE=='0000') {
					$("#msg").html("인증번호를 발송했습니다. SMS인증을 해주세요.");
					$("#reqSmsBtn").val("발송 완료");
					$("#reqSmsBtn").addClass("disabled_selfAuthPopup").off("click");
					$("#keycode").focus();
					reqSms = true;
				}
				else {
					alert("인증번호 발송을 실패 하였습니다. \n관리자에게 문의 바랍니다.");
					//alert(data.MSG);
					reqSms = false;
				}
			},
			error: function() {
				alert('오류가 발생하였습니다.');
				reqSms = false;
			}
		});
		
		return false;
	});
	
	 //OPT 인증
	$('#checkSmsBtn').click(function() {
		if(!reqSms) {
			alert('인증 요청을 해주세요.');
			return false;
		}
		if($('#keycode').val() == '') {
			alert('인증키를 입력해 주세요.');
			$('#keycode').focus();
			return false;
		}
		
		
		$.ajax({
			url: '/pororo/selfAuthentication/sms/auth',
			async: false,
			type: 'POST',
			data: {'keycode':$("#keycode").val()},
			dataType: 'json',
			success: function(data) {
				if (data.CODE=='0000') {
					$('#keycode').addClass("disabled_selfAuthPopup").attr("disabled","disabled");
					$("#msg").html("인증에 성공하였습니다.");
					$("#checkSmsBtn").val("인증 성공");
					$("#checkSmsBtn").addClass("disabled_selfAuthPopup").off("click");
					checkSms = true;
				}
				//SMS 인증번호 발송 실패
				else if ( data.CODE == '0020' ) {
					checkSms = false;
					alert('SMS 번호 발송에 실패 하였습니다.\n다시 시도해 주시기 바랍니다.');
					$('#keycode').val('');
				}
				//SMS 인증번호 틀림
				else if ( data.CODE == '0030' ) {
					checkSms = false;
					alert('SMS번호가 틀렸습니다. 다시한번 확인하여 주십시요.');
					$('#keycode').val('');
				}
				//SMS 인증번호 입력시간 초과
				else if ( data.CODE == '0031' ) {
					checkSms = false;
					alert('입력하신 SMS번호는 입력시간 초과로 사용 할 수 없습니다.\n다시 시도하여 주시기 바랍니다.');
					$('#keycode').val('');
				}
				else if ( data.CODE == '9998' ) {
					checkSms = false;
					alert('SMS필수 정보가 누락되었거나 인증키 정보가 없습니다.');
					$('#keycode').val('');
				}
				else {
					checkSms = false;
					alert('알 수 없는 오류가 발생 하였습니다.\n오류번호[' + data.CODE + '');
					$('#keycode').val('');
				}
			},
			error: function() {
				alert('오류가 발생하였습니다.');
				checkSms = false;
			}
		});
		
		return false;
	});	
	
	 //로그인
	$('#completBtn').click(function() {
		 
		if(!checkSms) {
			alert("SMS인증이 필요합니다.");
			$('#sms').focus();
			return false;
		}
		//$("#smsFrm").attr("action","/pororo/selfAuthentication/link");
		$("#smsFrm").submit();
		
		return false;
	});	
});

//휴대전화번호 검사
function _isValidPhoneNo() {
	var cellPhoneNo = jQuery.trim($("#ctn").val());
	
	if(cellPhoneNo == "") {
		alert(''.replaceAll('|||','\n').replaceAll('&nbsp;',' '));
		return false;
	}
	
	var regExp = /^01([0|1|6|7|8|9]?)-?([0-9]{3,4})-?([0-9]{4})$/;
	if(!regExp.test(cellPhoneNo)) {
		alert('휴대폰 번호가 올바르지 않습니다. 관리자에게 문의하여 올바른 휴대폰 번호를 등록해 주세요.');
		return false;
	}
	
	return true;
}
